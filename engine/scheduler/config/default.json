{
    "server": {
        "host": "localhost",
        "port": 4000,
        "timeout": 3600000
     },
     "rateLimiters": [{      
        "rateLimitWindowSec": 30,
        "rateLimitMaxHits": 300}],
    "agenda": {
        "loadBatchSize": 100,
        "DBName": "scheduler",
        "collectionName": "cronjobs",
        "processEvery": "30 seconds",
        "maxConcurrency": 20,
        "defaultConcurrency": 5,
        "lockLimit": 0,
        "defaultLockLimit": 0,
        "defaultLockLifetime": 600000
    },
    "database": {
        "maxPoolSize": 3
     },
     "cache": {
        "port": 6379
     },
     "queue": {
        "port": 5672,
        "reconnectInterval": 1000,
        "retryCount": 30
     },
     "general": {  
        "generalQueueCount": 1,
        "taskProcessQueueCount": 1,
        "platformBaseUrl": "http://platform-core-clusterip-service:4000",
        "maxPoolSize": 3
     }
}
