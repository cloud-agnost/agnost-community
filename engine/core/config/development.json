{
   "database": {
      "uri": "engine-mongodb-clusterip-service",
      "minPoolSize": 3
   },
    "cache": {
        "hostname": "engine-redis-clusterip-service",
        "port": 6379,
        "readReplica": {
            "hostname": "engine-redis-clusterip-service",
            "port": 6379
        }
     },
     "queue": {
        "hostname": "engine-rabbitmq",
        "port": 5672,
